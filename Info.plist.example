<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- URL Types for OAuth callback -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>pmr</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>pmr</string>
			</array>
		</dict>
	</array>
	
	<!-- Strava Configuration -->
	<key>STRAVA_CLIENT_ID</key>
	<string>173748</string> <!-- your numeric client id -->
	
	<key>STRAVA_BACKEND_EXCHANGE</key>
	<string>https://YOUR_WORKER.workers.dev/api/strava/exchange</string>
	
	<key>STRAVA_BACKEND_REFRESH</key>
	<string>https://YOUR_WORKER.workers.dev/api/strava/refresh</string>
	
	<!-- Required for OAuth to query Strava app -->
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>strava</string>
	</array>
</dict>
</plist>